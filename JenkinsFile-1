pipeline {
agent any

tools{
    maven 'Maven_Home'
    jdk '1.8.0'
}

stages{
    stage('Initialize') {  
        steps{
            git branch: 'master', url: 'https://github.com/vikramsingh-96/maven-project.git'
        }
    }
    stage('Build'){
        steps{
            sh 'mvn -B package'
        }

    }
}
}
